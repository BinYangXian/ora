-- 测试循环之while循环
declare

    v_sum number := 0;
    v_i number := 1;
begin

    /*
        while 循环
        
        while 条件 loop
            循环体
        end loop;
    */
    
    -- 计算1到100的累加和
    
    while v_i<=100 loop
        v_sum := v_sum+v_i;
        v_i :=v_i+1;
    end loop;
    
    dbms_output.put_line(v_sum);

end;
