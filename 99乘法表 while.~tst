-- 99³Ë·¨±í 
declare 
   s number :=1;
   i number :=1;
   j number :=1;
begin
    while i<=9 loop
      while j<=i loop
        s :=j*i;
        if s>=10 then 
          dbms_output.put(j||'*'||i||'='||s||' ');
        else
          dbms_output.put(j||'*'||i||'= '||s||' ');
        end if;
        j :=j+1;
      end loop;
      j :=1;
    dbms_output.new_line;
    i :=i+1;
   end loop;
 
end;
